            <kind>1</kind>
            <string>// Inherit entity variables
event_inherited();


// MOVE ENTITY CONSTANTS //

// Slope decceleration
S_SLOPE_SLOW = 0.5;


// MOVE ENTITY VARIABLES //

// Speed
hspd = 0;
vspd = 0;

// Previous frame position and speed
xPrev = x;
vPrev = y;
hspdPrev = 0;
vspdPrev = 0;

/* These variables have their respective speeds added to them at each
step.  The object is moved based on these rounded, but the un-used
fraction of a pixel is saved for next step, so objects with a
speed less than 1 will still gradually move. */
hCounter = 0;
vCounter = 0;

// Whether or not the player is on the ground
onGround = false;
</string>
