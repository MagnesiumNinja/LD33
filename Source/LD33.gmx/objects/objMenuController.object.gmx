buttons[0] = "Load Game";
buttons[1] = "New Game";
buttons[2] = "Quit";

numButtons = 3;

boxes[0] = "Fullscreen";
boxes[1] = "Speedrun";

numBoxes = 2;

selected = false;


var i;
for (i = 0; i &lt; numButtons; i++)
{
    var b = instance_create(MENU_START_X, MENU_START_Y + MENU_OFFSET * i, objMenuButton);
    b.text = buttons[i];
    b.selection = i;
}

for (i = 0; i &lt; numBoxes; i++)
{
    var b = instance_create(MENU_START_X - MENU_BOX_OFFSET, MENU_START_Y + MENU_OFFSET * 3 + CHECKBOX_Y_OFF * i, objMenuCheckbox);
    b.text = boxes[i];
    b.box = i;
    
    switch(i)
    {
        case 0: // FULLSCREEN
        {
            b.checked = global.FULLSCREEN;
        
            break;
        }
        
        case 1: // SPEEDRUN
        {
            b.checked = global.SPEEDRUN;
        
            break;
        }
    }
}
